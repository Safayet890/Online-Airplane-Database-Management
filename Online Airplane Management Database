SELECT [DepatureAirport]
	  ,[ArrivalAirport]
	  ,[DepatureDate]
      ,[AirplaneName]
      ,[Fare]
  FROM .[dbo].[Flight]
  WHERE DepatureAirport='bejing' 
		AND ArrivalAirport='wuhan' 
		AND Depaturedate='2021-01-28' 
		OR AirplaneName='china southen'
		AND Fare<400;
