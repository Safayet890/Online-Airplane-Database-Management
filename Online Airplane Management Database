SELECT City         
      ,[Country]
      ,[FlightCode]
from .[dbo].[Airport]

  WHERE City='Beijing   ' 
  AND Country =' China  ' 
  OR FlightCode ='qgbtyh   '
SELECT [DepatureAirport]	 
        ,[ArrivalAirport]
	  ,[DepatureDate]
      ,[AirplaneName]
      ,[Fare]
  FROM [dbo].[Flight]
  WHERE DepatureAirport=' Beijing ' 
		AND ArrivalAirport=' Singapore ' 
		AND DepatureDate=''5-28-2020' 
		OR AirplaneName='Scott Airlines'
		AND Fare<400;
SELECT PassportNumber
  ,[Name]
  ,[IDNumber]
        ,[PhoneNumber]
   
  FROM [dbo].[Passenger]
  WHERE PassportNumber='IG-36523 ' 
  AND Name=' PARVEZ ' 
  OR IDNumber='493882IY'
  AND PhoneNumber ='7955335'
SELECT OwnDiscount        
      ,[AirlanName]
      
from .[dbo].[Airline]

  WHERE OwnDiscount='3  ' 
  AND  AirlanName= 'Scoot Airlines'
