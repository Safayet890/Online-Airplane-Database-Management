SELECT TOP (1000) [AircraftNumber]
      ,[AircraftType]
  FROM [AirportDatabase].[dbo].[Airplane]
SELECT TOP (1000) [AirportCode]
      ,[City]
      ,[Country]
      ,[FlightCode]
  FROM [AirportDatabase].[dbo].[Airport]
SELECT TOP (1000) [FiightCode]
      ,[AirplaneName]
      ,[Airplanecode]
      ,[DepatureTime]
      ,[ArrivalTime]
      ,[TravelTime]
      ,[DepatureAirport]
      ,[ArrivalAirport]
      ,[Fare]
      ,[DepatureDate]
      ,[ArrivalDate]
  FROM [AirportDatabase].[dbo].[Flight]
SELECT TOP (1000) [PassportNumber]
      ,[Name]
      ,[IDNumber]
      ,[PhoneNumber]
  FROM [AirportDatabase].[dbo].[Passenger]
SELECT TOP (1000) [NumberStatus]
      ,[Type]
      ,[Status]
  FROM [AirportDatabase].[dbo].[Seat]
SELECT TOP (1000) [username]
      ,[password]
  FROM [AirportDatabase].[dbo].[useraccount]
SELECT [username]
      ,[password]
FROM [dbo].[useraccount]
where username='Shafayet' and password='1911562124';
SELECT TOP (1000) [FiightCode]
      ,[AirplaneName]
      ,[Airplanecode]
      ,[DepatureTime]
      ,[ArrivalTime]
  
      FROM [AirportDatabase].[dbo].[Flight]
SELECT TOP (1000) [AirportCode]
      ,[City]
      ,[Country]
      ,[FlightCode]
  FROM [AirportDatabase].[dbo].[Airport]
SELECT TOP (1000) [PassportNumber]
      ,[Name]
      ,[IDNumber]
      ,[PhoneNumber]
  FROM [AirportDatabase].[dbo].[Passenger]
