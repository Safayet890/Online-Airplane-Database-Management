USE airlinedatabases
CREATE TABLE [dbo].[Airport](
  [AirportCode][int] NOT NULL,
	[City] [char] (20) NULL,
	[Country] [char] (20) NULL,
	[FlightCode] [char] (20) NULL
)ON [PRIMARY]
CREATE TABLE [dbo]. [Anchor] (
	[AirportCode) [int) NOT NULL,
	[FlightCode] [char] (20) NOT NULL
) ON [PRIMARY]
CREATE TABLE [dbo] . [Flight] (
	[FiightCode] [char] (20) NOT NULL,
	[AirplaneName][char] (20) NULL,
	[Airplanecode][char] (10) NULL,
	[DepatureTime] [time] (7) NULL,
	[ArrivalTime] [time] (7) NULL,
	[TravelTime] [float] NULL,
	[DepatureAirport] [char] (20) NULL,
	[ArrivalAirport] [char] (20) NULL,
	[Fare] [float] NULL,
	[DepatureDate] [date] NULL,
	[ArrivalDate] [date] NULL
)	ON [PRIMARY]
CREATE TABLE [dbo] .[Order] (
	[orderNumber] [nchar] (20) NOT NULL,
	[startCity] [nchar] (20) NULL,
	[endCity] [nchar] (20) NULL,
	[price] [float] NULL,
	[startAirport] [nchar] (20) NULL,
	[endAirport][nchar] (20) NULL,
	[startDace] [date] NULL,
	[startTime] [time] (7) NULL,
	[endDate] [date] NULL,
	[endTime] [time] (7) NULL,
	[seatType] [nchar] (20) NULL,
CONSTRAINT [PK_Order] PRIMARY KEY CLUSTERED
(
	[orderNumber] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) ON [PRIMARY]
) ON [PRIMARY]
CREATE TABLE [dbo].[Passenger] (
	[PassportNumber] [char] (20) NOT NULL ,
	[Name] [char] (10) NULL,
	[IDNumber] [char] (18) NULL ,
	[PhoneNumber] [char] (20) NULL
)ON [PRIMARY]
CREATE TABLE [dbo]. [Airline] (
 [OwnDiscount] [int] NOT NULL,
 [AirlanName] [char] (40) NOT NULL
) ON [PRIMARY]
CREATE TABLE [dbo].[Airplane] (
[AircraftNumber] [character](20) NOT NULL ,
[AircraftType] [character] (20) NULL,
)ON [PRIMARY]
CREATE TABLE [dbo].[Seat] (
[NumberStatus] [integer] NOT NULL ,
[Type] [character] (20) NULL,
[Status] [integer]  NULL ,
)ON [PRIMARY]
CREATE TABLE [dbo].[useraccount] (
[username] [character] (20)NOT NULL ,
[password] [character] (20) NULL
)ON [PRIMARY]
